{
  "name": "toy-vscode-log-filter",
  "displayName": "toy_vscode_log_filter",
  "description": "log filter for operation trace",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/halfenif/toy_vscode_log_filter"
  },    
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toy-vscode-log-filter.logfilter",
        "title": "Log filter for DEBUG"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "toy-vscode-log-filter.logfilter",
          "when": "filesExplorerFocus",
          "group": "logfilter"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Log Filter Configuration",
      "properties": {
        "logfilter.filterCharacter": {
          "type": "string",
          "default": "*",
          "description": "Character to filter lines by."
        },
        "logfilter.position": {
          "type": "number",
          "default": 20,
          "description": "Compare start position (start 1)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0"
  }
}
